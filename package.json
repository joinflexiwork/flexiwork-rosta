{
  "name": "flexiwork-rosta",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "backup:mentlast": "powershell -ExecutionPolicy Bypass -File ./scripts/backup-to-mentlast.ps1",
    "backup:supabase": "node scripts/backup-supabase.js",
    "dev": "next dev",
    "dev:clean": "node -e \"try{require('fs').unlinkSync('.next/dev.lock');console.log('Removed .next/dev.lock');}catch(e){}\" && next dev",
    "dev:port": "next dev --port 3001",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest run --reporter=verbose",
    "test:quick": "vitest run --reporter=verbose",
    "test:db": "tsx scripts/test-db-connection.ts",
    "test:e2e": "playwright test --reporter=line",
    "test:all": "npm run test:quick && npm run test:db && npm run test:e2e"
  },
  "dependencies": {
    "@supabase/auth-helpers-nextjs": "^0.15.0",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.95.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "react": "19.2.3",
    "react-big-calendar": "^1.19.4",
    "react-dom": "19.2.3",
    "resend": "^6.9.1",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^5.15.8",
    "@playwright/test": "^1.49.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "jsdom": "^25.0.1",
    "tailwindcss": "^4",
    "tsx": "^4.19.2",
    "typescript": "^5",
    "vitest": "^2.1.6"
  }
}
